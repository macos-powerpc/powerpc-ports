From 68554ba380e16db6e94b8f7ea17e75cc3c4f413b Mon Sep 17 00:00:00 2001
From: Sergey Fedorov <vital.had@gmail.com>
Date: Mon, 21 Jul 2025 01:46:41 +0800
Subject: [PATCH 19/31] LibosxLibraries.gmk, PlatformLibraries.gmk: disable
 some unneeded macOS-specific components

---
 jdk/make/lib/LibosxLibraries.gmk   | 2 +-
 jdk/make/lib/PlatformLibraries.gmk | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/jdk/make/lib/LibosxLibraries.gmk b/jdk/make/lib/LibosxLibraries.gmk
index 14762e600e..8b349429cf 100644
--- a/jdk/make/lib/LibosxLibraries.gmk
+++ b/jdk/make/lib/LibosxLibraries.gmk
@@ -23,7 +23,7 @@
 # questions.
 #
 
-ifeq ($(OPENJDK_TARGET_OS), macosx)
+ifeq ($(OPENJDK_TARGET_OS), macosx_lion_up)
 
   ################################################################################
 
diff --git a/jdk/make/lib/PlatformLibraries.gmk b/jdk/make/lib/PlatformLibraries.gmk
index e1b83ce604..a339d0ee6f 100644
--- a/jdk/make/lib/PlatformLibraries.gmk
+++ b/jdk/make/lib/PlatformLibraries.gmk
@@ -25,7 +25,7 @@
 
 ################################################################################
 
-ifeq ($(OPENJDK_TARGET_OS), macosx)
+ifeq ($(OPENJDK_TARGET_OS), macosx_lion_up)
 
   LIBOSXAPP_SRC := $(JDK_TOPDIR)/src/java.desktop/macosx/native/libosxapp
 
