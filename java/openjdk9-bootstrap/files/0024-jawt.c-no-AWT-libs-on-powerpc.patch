+++ b/jdk/src/java.desktop/unix/native/libjawt/jawt.c
@@ -39,7 +39,7 @@ DEF_STATIC_JNI_OnLoad
  */
 JNIEXPORT jboolean JNICALL JAWT_GetAWT(JNIEnv* env, JAWT* awt)
 {
